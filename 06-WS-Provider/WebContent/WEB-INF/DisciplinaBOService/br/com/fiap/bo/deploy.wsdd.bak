<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DisciplinaBOService WSDL service -->

  <service name="DisciplinaBO" provider="java:RPC" style="document" use="encoded">
      <parameter name="wsdlTargetNamespace" value="http://bo.fiap.com.br"/>
      <parameter name="wsdlServiceElement" value="DisciplinaBOService"/>
      <parameter name="wsdlServicePort" value="DisciplinaBO"/>
      <parameter name="className" value="br.com.fiap.bo.DisciplinaBOSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="DisciplinaBO"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>

  </service>
</deployment>
